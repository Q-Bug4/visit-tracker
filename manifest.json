{
    "manifest_version": 3,
    "name": "Link History Checker",
    "version": "1.0",
    "description": "Displays the last visit time and visit count of links in the browsing history.",
    "permissions": [
      "history",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["popup.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  